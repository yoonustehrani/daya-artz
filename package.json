        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "resolve-url-loader": "^4.0.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.3",
        "animate.css": "^3.7.2",
        "bootstrap": "^4.6.0",
        "swiper": "^6.8.0"
    }
